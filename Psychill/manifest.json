{
  "manifest_version": 2,

  "name": "Psychill",
  "description": "Hours of Ambient Psychill music at the click of a button. Perfect for when studying or coding. ",
  "version": "0.0.2",

  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "index.html"
  },


"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

  "permissions": [
  "tabs", "https://www.youtube.com/*", 
  "<all_urls>",
  "activeTab" 
  ],

"background":{
        "scripts":["script/script.js"],
        "persistent": false
        }

}